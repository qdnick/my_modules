<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="view_mass_update_doctor_wizard_form" model="ir.ui.view">
        <field name="name">mass_update_doctor_wizard.form</field>
        <field name="model">mass.update.doctor.wizard</field>
        <field name="arch" type="xml">
            <form string="Mass Update Personal Doctor">
                <group>
                    <field name="new_doctor_id"/>
                </group>
                <footer>
                    <button name="action_update_doctor"
                            string="Update"
                            type="object"
                            class="btn-primary"
                            confirm="Do you realy wanted update a doctor?"
                    />
                    <button string="Cancel"
                            class="btn-secondary"
                            special="cancel"
                    />
                </footer>
            </form>
        </field>
    </record>

</odoo>
